
#[foreign]
func printf(format ptr<u8>, ...) i32

struct Person (firstname ptr<u8>, lastname ptr<u8>)

func main {
    john := Person {
        firstname: c"John",
        lastname: c"Smith",
    }

    printf(c"Welcome, %s %s\n", john.firstname, john.lastname)

    jane := Person {
        firstname: c"Jane",
        lastname: john.lastname,
    }

    printf(c"Welcome, %s %s\n", jane.firstname, jane.lastname)
}

