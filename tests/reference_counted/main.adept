
#[foreign]
func printf(format ptr<u8>, ...) i32

struct Person (firstname ptr<u8>, lastname ptr<u8>)

func main {
    person := Person {
        firstname: c"John",
        lastname: c"Smith",
    }

    printf(c"Welcome, %s %s\n", person.firstname, person.lastname)

    person.firstname = c"Jane"
    printf(c"Welcome, %s %s\n", person.firstname, person.lastname)
}

