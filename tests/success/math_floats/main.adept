
pragma => adept("3.0")

#[foreign]
func printf(format ptr<char>, ...) int

func main {
    a := 13.14
    b := 8.76543

    printf(c"%f + %f = %f\n", a, b, a + b)
    printf(c"%f - %f = %f\n", a, b, a - b)
    printf(c"%f * %f = %f\n", a, b, a * b)
    printf(c"%f / %f = %f\n", a, b, a / b)
    printf(c"%f %% %f = %f\n", a, b, a % b)
}

