
pragma => adept("3.0")

#[foreign]
func printf(format ptr<char>, ...) int

func main {
    a := 100
    printf(c"!%lld = %lld\n", a, !a)
    printf(c"-%lld = %lld\n", a, -a)
    printf(c"~%lld = %lld\n", a, ~a)
    printf(c"!!%lld = %lld\n", a, !!a)
    printf(c"--%lld = %lld\n", a, --a)
    printf(c"~~%lld = %lld\n", a, ~~a)
}

