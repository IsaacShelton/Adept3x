
pragma => adept("3.0")

#[foreign]
func printf(format ptr<char>, ...) int

func main {
    a := 13
    b := 8

    printf(c"%lld & %lld = %lld\n", a, b, a & b)
    printf(c"%lld | %lld = %lld\n", a, b, a | b)
    printf(c"%lld ^ %lld = %lld\n", a, b, a ^ b)
    printf(c"%lld >> %lld = %lld\n", a, b, a >> b)
    printf(c"%lld >>> %lld = %lld\n", a, b, a >>> b)
    printf(c"%lld << %lld = %lld\n", a, b, a << b)
    printf(c"%lld <<< %lld = %lld\n", a, b, a <<< b)
}

