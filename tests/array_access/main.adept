
#[foreign]
{
    func printf(format ptr<char>, ...) int
    func malloc(size ulonglong) ptr<void>
    func free(pointer ptr<void>) void
}

func main {
    items ptr<i32> = malloc(4 * 10)

    i i32 = 0
    while i < 10 {
        items[i] = 10 * i
        i = i + 1
    }

    i i32 = 0
    while i < 10 {
        printf(c"items[%d] = %d\n", i, items[i])
        i = i + 1
    }

    free(items)
}

